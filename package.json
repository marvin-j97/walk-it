{
  "name": "walk-it",
  "version": "0.1.0",
  "description": "File walk iterator",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node build",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix --ext .ts",
    "test": "vitest run",
    "test:open": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marvin-j97/walk-it.git"
  },
  "keywords": [
    "file",
    "walk",
    "iterator",
    "recursive"
  ],
  "author": "marvin-j97",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marvin-j97/walk-it/issues"
  },
  "homepage": "https://github.com/marvin-j97/walk-it#readme",
  "devDependencies": {
    "@types/node": "12",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "prettier": "2.7.1",
    "typescript": "4.7.4",
    "vitest": "0.22.1",
    "@vitest/coverage-istanbul": "0.22.1",
    "@vitest/ui": "0.23.4",
    "tsup": "6.2.2"
  },
  "engines": {
    "node": ">=12"
  }
}
