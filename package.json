{
  "name": "walk-it",
  "version": "3.0.0",
  "license": "MIT",
  "description": "Async file walk iterator",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "lint": "rome check src",
    "lint:fix": "rome check --apply-unsafe src",
    "fmt": "rome format src --write",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marvin-j97/walk-it.git"
  },
  "keywords": [
    "file",
    "walk",
    "iterator",
    "recursive"
  ],
  "author": "marvin-j97",
  "bugs": {
    "url": "https://github.com/marvin-j97/walk-it/issues"
  },
  "homepage": "https://github.com/marvin-j97/walk-it#readme",
  "devDependencies": {
    "@types/node": "16.18.38",
    "@vitest/coverage-istanbul": "0.34.6",
    "@vitest/ui": "0.34.6",
    "rome": "12.1.3",
    "tsup": "7.2.0",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  },
  "engines": {
    "node": "16 || 18 || 20"
  }
}
