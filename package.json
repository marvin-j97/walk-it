{
  "name": "walk-it",
  "version": "4.1.0",
  "license": "MIT",
  "description": "Async file walk iterator",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./builder": {
      "types": "./dist/builder.d.ts",
      "import": "./dist/builder.mjs"
    }
  },
  "scripts": {
    "lint": "biome check src",
    "lint:fix": "biome check --apply-unsafe src",
    "fmt": "biome format src --write",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marvin-j97/walk-it.git"
  },
  "keywords": [
    "file",
    "walk",
    "iterator",
    "recursive"
  ],
  "author": "marvin-j97",
  "bugs": {
    "url": "https://github.com/marvin-j97/walk-it/issues"
  },
  "homepage": "https://github.com/marvin-j97/walk-it#readme",
  "devDependencies": {
    "@types/node": "18.18.6",
    "@vitest/coverage-istanbul": "0.34.6",
    "@vitest/ui": "0.34.6",
    "@biomejs/biome": "1.3.1",
    "tsup": "7.2.0",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  },
  "engines": {
    "node": "18 || 20"
  }
}
