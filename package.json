{
  "name": "walk-it",
  "version": "5.0.1",
  "license": "MIT",
  "description": "Async file walk iterator",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "biome:check": "biome check src",
    "lint": "biome lint src",
    "lint:fix": "biome lint --apply src",
    "fmt": "biome format src --write",
    "fmt:check": "biome format src",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "build": "tsup src/index.ts --format esm --dts --clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marvin-j97/walk-it.git"
  },
  "keywords": [
    "file",
    "walk",
    "iterator",
    "recursive"
  ],
  "author": "marvin-j97",
  "bugs": {
    "url": "https://github.com/marvin-j97/walk-it/issues"
  },
  "homepage": "https://github.com/marvin-j97/walk-it#readme",
  "devDependencies": {
    "@biomejs/biome": "1.7.1",
    "@types/node": "18.19.61",
    "@vitest/coverage-istanbul": "1.6.0",
    "tsup": "8.0.2",
    "typescript": "5.4.5",
    "vite": "5.2.10",
    "vitest": "1.6.0"
  },
  "engines": {
    "node": "18 || 20 || 22"
  }
}
